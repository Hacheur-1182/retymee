        <!-- Modals Connexion for students-->
        <div class="modal fade" id="log" tabindex="-1" role="dialog" aria-labelledby="exampleModalLabel" aria-hidden="true">
            <div class="modal-dialog" role="document">
                <div class="modal-content">
                <div class="modal-header">
                    <h5 class="modal-title text-center" id="exampleModalLabel">Connectez vous à votre compte! </h5>
                    <button type="button" class="close" data-dismiss="modal" aria-label="Close">
                    <span aria-hidden="true">&times;</span>
                    </button>
                </div>
                <div class="modal-body">
                    <div class="container text-center">
                        <div class="row">
                            <div class="col-md-12">

                                <!-- <button href="" id="facebook" class="btn btn-block btn-primary">
                                    <i class="fa fa-facebook"></i>&nbsp;&nbsp;&nbsp;&nbsp;&nbsp;&nbsp; Continue with Facebook
                                </button>
                                <button id="google" href="" class="btn btn-block btn-primary">
                                    <i class="fa fa-google-plus"></i>&nbsp;&nbsp;&nbsp;&nbsp;&nbsp;&nbsp; Continue with Google
                                </button>
                                <h6 class="py-2">OR</h6> -->
                                <form  method="post" action="/student/login" id="form-login" data-parsley-validate>
                                    <div class="form-group">
                                        <div class="input-group">
                                            <span class="input-group-addon">
                                                Adresse Email
                                            </span>
                                            <input type="email" id="login_email" data-parsley-errors-container="#a" name="login_email" placeholder="Adresse Email" class="form-control" data-parsley-required>
                                        </div><span class="text-danger" id="a"></span>
                                    </div>
                                    <div class="form-group">
                                        <div class="input-group">
                                            <span class="input-group-addon">
                                                Mot de passe
                                            </span>
                                            <input type="password" id="login_password" name="login_password" data-parsley-errors-container="#b" placeholder="Mot de passe" class="form-control" data-parsley-required>
                                        </div><span class="text-danger" id="b"></span>
                                    </div>
                                    <button type="submit" id="login" class="btn btn-block button-yellow" >Se Connecter</button>
                                </form><br>
                                <div class="form-check mb-2 mr-sm-2">
                                    <input class="form-check-input" type="checkbox" name="isTeatcher" id="isTeatcher" placeholder="Je suis un enseignant" value="Je suis un enseignant" />
                                    <label class="form-check-label" for="isTeatcher">
                                        Je suis un enseignant
                                    </label>
                                </div>
                                <h6><a href="/user/reset-password">Mot de passe oublié ?</a></h6>
                            </div>
                        </div>
                    </div>
                </div>
                <div class="modal-footer justify-content-center">
                    <div class="row py-0">
                        <div class="col py-0">
                            <h6><strong>Vous n'avez pas de compte? <a href="" id="sign-in-log" data-toggle="modal" data-target="#signUp">Inscrivez vous</a> </strong></h6>
                        </div>
                    </div>
                </div>
                </div>
            </div>
        </div>

         <!-- Modals Inscription-->
        <div class="modal fade" id="signUp" tabindex="-1" role="dialog" aria-labelledby="exampleModalLabel" aria-hidden="true">
            <div class="modal-dialog" role="document">
                <div class="modal-content">
                    <div class="modal-header">
                        <h5 class="modal-title" id="exampleModalLabel">Inscription</h5>
                        <button type="button" class="close" data-dismiss="modal" aria-label="Close">
                        <span aria-hidden="true">&times;</span>
                        </button>
                    </div>
                    <div class="modal-body">
                        <div class="container text-center">
                            <div class="row">
                                <div class="col">

                                    <!-- <button href="" id="facebook" class="btn btn-block btn-primary">
                                        <i class="fa fa-facebook"></i>&nbsp;&nbsp;&nbsp;&nbsp;&nbsp;&nbsp; Sign Up with Facebook
                                    </button>
                                    <button id="google" href="" class="btn btn-block btn-primary">
                                        <i class="fa fa-google-plus"></i>&nbsp;&nbsp;&nbsp;&nbsp;&nbsp;&nbsp; Sign Up with Google
                                    </button><br> -->
                                    <form class="pb-4"  id="form-register" data-parsley-validate>
                                        <div class="form-group">
                                            <div class="input-group">
                                                <span class="input-group-addon">
                                                    Nom Complet
                                                </span>
                                                <input type="text" id="username" data-parsley-errors-container="#aa"  placeholder="Votre Nom" name="username" class="form-control" data-parsley-required data-parsley-minlength="5" ><br>
                                            </div><span class="text-danger" id="aa"></span>
                                        </div>
                                        <div class="form-group">
                                            <div class="input-group">
                                                <span class="input-group-addon">
                                                    Adresse Email
                                                </span>
                                                <input type="email" name="login_email" id="email" placeholder="Adresse Email" data-parsley-errors-container="#bb" class="form-control" data-parsley-required>
                                            </div><span class="text-danger" id="bb"></span>
                                        </div>
                                        <div class="form-group">
                                            <span class="text-left" style="display: block;">Type d'apprenant</span>
                                            <select name="status" id="status" class="form-control">
                                                <option>Elève</option>
                                                <option>Etudiant</option>
                                                <option>Professionel</option>
                                            </select>
                                        </div>
                                        <div class="form-group">
                                            <div class="input-group">
                                                <span class="input-group-addon">
                                                    Mot de passe
                                                </span>
                                                <input type="password" id="password" 
                                                    data-parsley-errors-container="#cc" 
                                                    placeholder="Mot de passe" 
                                                    name="login_password" 
                                                    class="form-control" 
                                                    data-parsley-required data-parsley-minlength="6"
                                                    data-parsley-uppercase="1"
                                                    data-parsley-lowercase="1"
                                                    data-parsley-number="1"
                                                    data-parsley-special="1">
                                            </div><span class="text-danger" class="text-danger" id="cc"></span>
                                        </div>
                                        <div class="form-group">
                                            <div class="input-group">
                                                <span class="input-group-addon">
                                                    Confirmer le mot de passe
                                                </span>
                                                <input type="password" id="password2" name="password2" data-parsley-errors-container="#dd" placeholder="Confirmation" class="form-control" data-parsley-required  data-parsley-equalto="#password">
                                            </div><span class="text-danger" id="dd"></span>
                                        </div>
                                        <button type="submit" id="signup" class="btn btn-block button-yellow" href="#">S'inscrire</button>
                                    </form>
                                </div>
                            </div>
                        </div>
                    </div>
                </div>
            </div>
        </div>

        <!--Request Demo Modal-->
        <div class="modal fade" id="see-demo" tabindex="-1" role="dialog" aria-labelledby="exampleModalLabel" aria-hidden="true">
