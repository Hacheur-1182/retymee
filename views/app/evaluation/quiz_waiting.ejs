<%- include('../../_layout/dashboard-header') %>
<script src="//cdnjs.cloudflare.com/ajax/libs/moment.js/2.7.0/moment.min.js" type="text/javascript"></script>

    <section id="header" style="margin-left: 3em">
        <div class="container-fluid py-4">
            <div class="row pt-3 pb-2">
                <div class="col-md-12 text-center d-flex justify-content-center align-items-center">
                    <%if(user.image != "avatar.png"){%>
                        <img src="/img/student_images/<%=user._id%>/<%=user.image%>" class="img-rounded" alt="">
                    <%}else{%>
                        <img src="/img/avatar.png" class="img-rounded" alt="">
                    <%}%>
                    <div class="d-flex flex-column justify-content-center align-items-center">
                        <%if(user.firstname != ""){%>
                            <p class="text-white ml-4 mb-0"><b><%=user.email%></b></p>
                        <%}%>
                        
                        <p class="text-white ml-4 mb-0"><b><%=user.email%></b></p>

                    </div>
                </div>
            </div>
        </div>
    </section>
    

    <section id="quiz-dashboard" class="pb-5 pt-5">
        <div class="container-fluid">
            <div class="row my-3 justify-content-center">
                <div class="col-sm-12 mt-2 mb-4 d-flex flex-column justify-content-center align-items-center">
                    <h5 class="pb-0 mb-0 text-center mt-4">LE QUIZ N'EST PAS ENCORE DISPONIBLE A CE MOMENT!</h5>
                    <a class="btn button-yellow mt-3" href="/student/dashboard">Retounez au Tableau de bord</a>
                </div>
            </div>
        </div>
    </section>

    <%- include('../../_layout/dashboard-footer')%>